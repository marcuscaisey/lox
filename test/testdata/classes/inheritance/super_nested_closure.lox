class Superclass {
  toString() {
    return "Superclass";
  }
}

class Subclass < Superclass {
  getClosure() {
    fun f() {
      fun g() {
        fun h() {
          return super.toString();
        }
        return h;
      }
      return g;
    }
    return f;
  }
}

var closure = Subclass().getClosure();
print closure()()(); // prints: Superclass
