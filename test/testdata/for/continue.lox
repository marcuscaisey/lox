  print i;
}

for (var j = 0; j < 3; j = j + 1) {
  for (var k = 0; k < 2;) {
    k = k + 1;
    if (k == 2) {
      continue;
    }
    // prints: 0
    // prints: 1
    // prints: 2
    print j;
  }
}
