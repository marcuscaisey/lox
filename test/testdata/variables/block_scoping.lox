var a = "global a";
var b;
var c = "global c";

{
  print a; // prints: global a
  b = "global b";
  print b; // prints: global b
  var c = "outer c";
  print c; // prints: outer c

  var d = "outer d";
  var e;
  var f = "outer f";

  {
    print d; // prints: outer d
    e = "outer e";
    print e; // prints: outer e
    var f = "inner f";
    print f; // prints: inner f
  }

  print e; // prints: outer e
  print f; // prints: outer f
}

print b; // prints: global b
print c; // prints: global c
