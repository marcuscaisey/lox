// Prints the nth fibonacci number where n is passed as an argument to the script.

fun fib(n) {
  if (n < 2) {
    return n;
  }
  return fib(n - 1) + fib(n - 2);
}

if (argv.length < 2) {
  error("argument not provided");
}
if (argv.length > 2) {
  error("too many arguments provided");
}

var n = parseNumber(argv[1]);
print fib(n);
